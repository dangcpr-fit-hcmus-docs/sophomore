test.leq.oneside <- function(x,y,mu0,alpha) {
D = x-y
n = length(D)
D.bar = mean(D)
D.sd = sd(D)
t0 = (D.bar-mu0)*sqrt(n)/D.sd
p.value = pt(t0,n-1)
cat("Voi muc y nghia ",100*alpha, "% \n")
if(p.value < alpha)
cat("Bac bo H0 voi p-gia tri=", p.value, "\n")
else
cat("Chua du co so de bac bo H0 voi p-gia tri=",p.value,"\n")
}
x = Before; y = After
#Ap dung mu0=0
